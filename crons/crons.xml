<odoo>
    <data>
        <record model="ir.cron" id="kill4city_cron_conquer_update">
            <field name="name">kill4city: Cron conquer Update</field>
            <field name="model_id" ref="model_kill4city_conquer" />
            <field name="state">code</field>
            <field name="code">model.update_conquer()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="activity_user_type">specific</field>
            <field name="doall" eval="False" />
        </record>
    </data>
    <data>
        <record model="ir.cron" id="kill4city_cron_training_update">
            <field name="name">kill4city: Cron training Update</field>
            <field name="model_id" ref="model_kill4city_training" />
            <field name="state">code</field>
            <field name="code">model.training_process()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="activity_user_type">specific</field>
            <field name="doall" eval="False" />
        </record>
    </data>
</odoo>